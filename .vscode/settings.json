{
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "files.eol": "\n",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/node_modules/**": true,
        ".tmp": true
    },
    "files.exclude": {
        ".tmp": true
    },
    "search.exclude": {
        ".tmp": true,
        "typings": true
    },
    "json.schemas": [
        {
            "fileMatch": [
                "/pbiviz.json"
            ],            
            "url": "./.api/v1.6.0/schema.pbiviz.json"
        },
        {
            "fileMatch": [
                "/capabilities.json"
            ],            
            "url": "./.api/v1.6.0/schema.capabilities.json"
        },
        {
            "fileMatch": [
                "/dependencies.json"
            ],            
            "url": "./.api/v1.6.0/schema.dependencies.json"
        }        
    ],
    "workbench.colorCustomizations": {
        "commandCenter.border": "#e7e7e799",
        "panel.border": "#3333ff",
        "titleBar.activeBackground": "#0000ff",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#0000ff99",
        "titleBar.inactiveForeground": "#e7e7e799"
    }
}